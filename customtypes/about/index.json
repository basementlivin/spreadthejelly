              "type": "SharedSlice"
            },
            "testimonials": {
              "type": "SharedSlice"
            },
            "pull_quote": {
              "type": "SharedSlice"
            },
            "recommendations": {
              "type": "SharedSlice"
            },
            "headline": {
              "type": "SharedSlice"
            },
            "spacer": {
              "type": "SharedSlice"
